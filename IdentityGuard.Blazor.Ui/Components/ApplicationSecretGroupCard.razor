<MudCard>
    <MudCardHeader>
        <MudText Typo="Typo.subtitle1">@Title</MudText>
    </MudCardHeader>
    <MudCardContent>
        <MudList>
            @foreach(var item in Data.OrderBy(i=>i.DisplayName))
            {
                <MudListItem>
                    <MudText Typo="Typo.subtitle1">@item.DisplayName</MudText>
                    <MudText Typo="Typo.body1">Expires: @item.ExpiresAt</MudText>
            </MudListItem>
            }
        </MudList>
    </MudCardContent>
</MudCard>